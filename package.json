{
  "name": "potato-bruce",
  "version": "1.0.0",
  "private": true,
  "homepage": "https://potatobruce.io",
  "scripts": {
    "clean": "rimraf dist st-types",
    "prebuild": "npm run clean",
    "build": "webpack --mode production --no-devtool",
    "start": "webpack serve --progress --open",
    "st:watch": "stc --cjs false --dts true -w",
    "serve": "serve ./dist -s",
    "lint": "eslint",
    "typecheck": "tsc --noEmit",
    "test": "npm run build && npm run lint && npm run typecheck",
    "prettify": "npx prettier . --write",
    "predeploy": "npm run build",
    "deploy": "ncp ./CNAME ./dist/CNAME && gh-pages -d ./dist"
  },
  "dependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@stylable/cli": "^6.1.1",
    "@stylable/core": "^6.1.1",
    "@stylable/webpack-plugin": "^6.1.1",
    "@types/react": "^19.0.2",
    "@types/react-dom": "^19.0.2",
    "@wixc3/react-board": "^4.6.2",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.1.0",
    "gh-pages": "^6.2.0",
    "html-webpack-plugin": "^5.6.3",
    "ncp": "^2.0.0",
    "rimraf": "^6.0.1",
    "serve": "^14.2.4",
    "ts-loader": "^9.5.1",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.18.0",
    "webpack": "^5.97.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.2.0"
  }
}
